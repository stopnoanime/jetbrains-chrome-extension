{
  "manifest_version": 3,
  "name": "Network Request Tracker",
  "description": "JetBrains internship task",
  "version": "1.0",
  "action": {
    "default_popup": "src/popup.html"
  },
  "background": {
    "service_worker": "src/service-worker.js",
    "type": "module"
  },
  "permissions": [
    "storage",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}